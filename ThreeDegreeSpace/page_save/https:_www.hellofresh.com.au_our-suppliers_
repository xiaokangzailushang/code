<script>
    var hellofresh = hellofresh || {};
    hellofresh.$loginForm = $("#login-section-form");
    hellofresh.$registerForm = $("#register-section-form");
    hellofresh.$forgotForm = $("#forgot-section-form");

    hellofresh.showLogin = function () {
        hellofresh.$loginForm.show().siblings().hide();
        hellofresh.trackLogin("modal login viewed");
    }

    hellofresh.showRegister = function () {
        hellofresh.$registerForm.show().siblings().hide();
        hellofresh.trackLogin("modal register viewed");
    }

    hellofresh.showForgot = function () {
        hellofresh.$forgotForm.show().siblings().hide();
        hellofresh.trackLogin("modal forgot viewed");
    }

    hellofresh.trackLogin = function(eventAction) {
        dataLayer.push({
            'event' : 'gaEventTrigger',
            'gaEventCategory' : 'login',
            'gaEventAction' : eventAction,
            'gaEventLabel' : location.pathname
        });
        //console.log("tracking action .. " + eventAction);
    }

    $("#loginModal").on('hidden.bs.modal', function (){
        hellofresh.$loginForm.show().siblings().hide();
        hellofresh.trackLogin("modal closed");
    });

    $("#loginModal").on('show.bs.modal', function (){
        hellofresh.trackLogin("modal open");
    });

    $("#loginModal").on('shown.bs.modal', function (){
        hellofresh.trackLogin("modal login viewed");
    });

    $('.toggle-input-type').on('change', function(){
        var $this = $(this);
        var showPassword = $this.prop('checked');
        if(typeof showPassword === "boolean" && showPassword){
            $this.closest('.input-group').children('.form-control').attr('type', 'text');
        }else{
            $this.closest('.input-group').children('.form-control').attr('type', 'password');
        }
    });

</script>


        </div> <!-- wrapper -->
    <script type="text/javascript">jQuery(document).ready(function (){jQuery(function($) {
jQuery('body').delegate('#yt0','click',function(){jQuery.ajax({'type':'POST','dataType':'json','success':function(data) {
            if(data.status=="success"){
                hellofresh.trackLogin("modal login success");
                setTimeout(function() {
                    window.location.href = data.redirectTo;
                },50);
            } else {
                hellofresh.trackLogin("modal login failed");
                $.each(data, function(key, val) {
                    $("#"+key+"_em_").text(val);
                    $("#"+key+"_em_").show();
                    $(".login-button").removeAttr("disabled");
                });
            }
        },'beforeSend':function(){
            hellofresh.trackLogin("modal login clicked");
            $("#login-section-form .form-error").hide();
            $(".login-button").attr("disabled","disabled");
        },'url':'/account/user/login/','cache':false,'data':jQuery(this).parents("form").serialize()});return false;});
$('#login-section-form').yiiactiveform({'validateOnSubmit':true,'validateOnChange':false,'hideErrorMessage':true,'attributes':[{'inputID':'LoginForm_email','errorID':'LoginForm_email_em_','model':'LoginForm','name':'email'},{'inputID':'LoginForm_password','errorID':'LoginForm_password_em_','model':'LoginForm','name':'password'}]});
jQuery('body').delegate('#yt1','click',function(){jQuery.ajax({'type':'POST','dataType':'json','success':function(data) {
            if(data.status=="success"){
                hellofresh.trackLogin("modal register success");
                setTimeout(function() {
                    window.location.href = data.redirectTo;
                },50);
            } else {
                hellofresh.trackLogin("modal register failed");
                $.each(data, function(key, val) {
                    $("#"+key+"_em_").text(val);
                    $("#"+key+"_em_").show();
                });

                $(".register-button").removeAttr("disabled");
            }
        },'beforeSend':function(){
            hellofresh.trackLogin("modal register clicked");
            $("#register-section-form .form-error").hide();
            $(".register-button").attr("disabled","disabled");
        },'url':'/account/user/register/','cache':false,'data':jQuery(this).parents("form").serialize()});return false;});
$('#register-section-form').yiiactiveform({'validateOnSubmit':true,'validateOnChange':false,'hideErrorMessage':true,'attributes':[{'inputID':'RegistrationForm_gender','errorID':'RegistrationForm_gender_em_','model':'RegistrationForm','name':'gender'},{'inputID':'RegistrationForm_first_name','errorID':'RegistrationForm_first_name_em_','model':'RegistrationForm','name':'first_name'},{'inputID':'RegistrationForm_last_name','errorID':'RegistrationForm_last_name_em_','model':'RegistrationForm','name':'last_name'},{'inputID':'RegistrationForm_email','errorID':'RegistrationForm_email_em_','model':'RegistrationForm','name':'email'},{'inputID':'RegistrationForm_password','errorID':'RegistrationForm_password_em_','model':'RegistrationForm','name':'password'},{'inputID':'RegistrationForm_birthday','errorID':'RegistrationForm_birthday_em_','model':'RegistrationForm','name':'birthday'}]});
jQuery('body').delegate('#forgot-password','click',function(){jQuery.ajax({'type':'POST','dataType':'json','success':function(data) {
            if(data.status=="success"){
                hellofresh.trackLogin("modal forgot success");
                setTimeout(function() {
                    window.location.href = data.redirectTo;
                },50);

            } else {
                hellofresh.trackLogin("modal forgot failed");
                $.each(data, function(key, val) {
                    $("#"+key+"_em_").text(val);
                    $("#"+key+"_em_").show();
                    $(".forgot-password").removeAttr("disabled");
                });
            }
        },'beforeSend':function(){
            hellofresh.trackLogin("modal forgot clicked");
            $("#forgot-section-form .form-error").hide();
            $(".forgot-password").attr("disabled","disabled");
        },'url':'/account/user/forgotpassword/','cache':false,'data':jQuery(this).parents("form").serialize()});return false;});
$('#forgot-section-form').yiiactiveform({'validateOnSubmit':true,'validateOnChange':false,'hideErrorMessage':true,'attributes':[{'inputID':'ForgotPasswordForm_email','errorID':'ForgotPasswordForm_email_em_','model':'ForgotPasswordForm','name':'email'}]});
});})</script>
