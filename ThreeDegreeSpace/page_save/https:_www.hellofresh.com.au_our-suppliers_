<script type="text/javascript">
$(function() {
    dataLayer.push({'event' : 'formLeadGenBarShow'});
    $form = $('#leadsNewsletterForm');
    $form.on('submit', function() {
        $.ajax({
            type: "POST",
            url: "https://www.hellofresh.com.au/newsletter/signupajax/?ajax=true&type=index",
            data: {
                'YII_CSRF_TOKEN': __token,
                'email': $form.find('#leadsEmail').val().trim(),
            },
            dataType: 'json',
            beforeSend: function() {
                $form.find('.footer_newsletter_error').hide();
                $form.find('.btn-hover').children('.glyphicon').removeClass('glyphicon-chevron-right').addClass('glyphicon-refresh spin');
            },
            success: function(data) {
                if ( data.validation == false ) {
                    $form.find(".lead-form").hide().siblings('.lead-feedback').html('<p class="mbm">'+data.error+'</p>').show();
                    setTimeout(function() {
                        $form.find(".lead-feedback").hide().siblings('.lead-form').show();
                    }, 5000);

                } else {

                    $form.find(".lead-form").hide().siblings('.lead-feedback').html('<p class="mbm">'+'Congratulations! You&#039;ve successfully signed up.'+'</p>').show();
                    dataLayer.push({'event' : 'formLeadGenBarSubmit'});
                    createCookie('hfleadgen','1',365);
                    setTimeout(function() {
                        $form.slideUp();
                    }, 5000);
                }
            },
            error: function(data) {
                $form.find(".lead-form").hide().siblings('.lead-feedback').html('<p class="mbm">'+'There has been an issue adding you to our newsletter. Please try again'+'</p>').show();
                setTimeout(function() {
                    $form.find(".lead-feedback").hide().siblings('.lead-form').show();
                }, 5000);
            },
            complete: function() {
                $form.find('.btn-hover').children('.glyphicon').removeClass('glyphicon-refresh spin').addClass('glyphicon-chevron-right');
            }
        });

        return false;
    });

    $form.on('click', '.js-lead-close', function(event) {
        createCookie('hfleadgen','1',365);
        $form.slideUp();
    })
});


</script>


