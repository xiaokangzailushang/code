        ac.hide();
        $('#query').blur();
    });

    //toggling classes on search click
    jQuery(document).ready(        
    function()
    {  
       $(".autocomplete_ssearch").focus(function()
       {
            $("#sitesearch").addClass("sitesearchOver");            
            $("#sitesearch").removeClass("sitesearch");  
       });
       
       $(".autocomplete_ssearch").blur(function()
       {        
            $("#sitesearch").addClass("sitesearch");            
            $("#sitesearch").removeClass("sitesearchOver");            
           
       });
   });


</script>



