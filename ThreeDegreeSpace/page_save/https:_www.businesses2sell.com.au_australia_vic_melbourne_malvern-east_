		   $('#list_grid_tabs_id').addClass('grids_listing_items_cont');*/
		   $('.detail_head_list_container').css('visibility','visible');
     $('#list_grid_tabs_id').removeClass('grids_listing_items_cont');
	   $('#list_grid_tabs_id').addClass('detail_listing');
            });
    $('.box_type_list').click(function(e) {
		$('.vertical_list').removeClass('tab_list_active');
       $('.box_type_list').addClass('tab_list_active');
       $('.detail_head_list_container').css('visibility','hidden');
             $('#list_grid_tabs_id').removeClass('detail_listing');
	   $('#list_grid_tabs_id').addClass('grids_listing_items_cont');   
            });
        });
