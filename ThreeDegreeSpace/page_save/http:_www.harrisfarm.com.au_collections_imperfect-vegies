          //$("#product-932596937 .caption .salecontainer").addClass("is-special");          
          $("#product-932596937 .caption .unit_price").addClass("is-special");            
        }
            
        if (!compareAtPrice){
          if (uom != "" && uom ){ 
      $("#product-932596937 .caption .salecontainer .sale .compare_at_price").html('<small>$'+((price*conversionFactor)/100).toFixed(2)+" "+uom+'</small>');
          } 
        }else{
        $("#product-932596937 .caption .salecontainer .sale .compare_at_price").html('<small>$' + (variantArray[6]/100).toFixed(2) + ' per kg</small>');              
        }
              
       
          
        if(parseInt(variantArray[7])<=0){
          $("#product-932596937 .ajax-add-to-cart-quick").html("<p class='error'>Out of Stock</p>");          
          $("#product-932596937 a").css({"pointer-events":"none"});          
          $("#product-932596937").remove();
            
         }else{
            $("#product-932596937").addClass("in-stock");
            
            var items = $("#headercartvals").attr("data-items").split(";");
  
            $.each(items, function(index, item) {
              if (item){ 
                  var itemId = item.split("-")[0];
                  var itemQty = item.split("-")[1];
                 if(variantArray[4] === itemId){

                     // console.log(variantArray[4] + " in cart");
           $("#product-932596937").find(".ajax-add-to-cart-quick").hide();
                     $("#product-932596937").find(".ajax-update-cart .addToCartInput").val(itemQty);
           $("#product-932596937").find(".ajax-update-cart").show();

                     return;
                       
                  } 
                }
      });
        
            
            
         }      
     }
            
     
                                 
            
   }
                   
       
$(".product-item").not(".in-stock").hide();
              
</script>




        
           



  






