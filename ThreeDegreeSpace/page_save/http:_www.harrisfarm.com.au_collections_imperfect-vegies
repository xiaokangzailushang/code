<script>        
  var cookieStore;  
  if($.cookie("picking_store")==null){
    cookieStore = 48;
  }else{
    cookieStore = $.cookie("picking_store");
  }
  
  var pickingStore = "s:"+cookieStore;
  
  for (var i = 0; i < partsOfStr.length; i++) {
    var variantArray = partsOfStr[i].split(',');
  var showProduct = false;
    //console.log("V: "+variantArray[4]+" |DATA: " + variantArray[0] + " |PICK: " + pickingStore);
    
    if(variantArray[0].trim() == pickingStore.trim()){  
      showProduct = true;
      var special =  variantArray[2]; 
      var additional = variantArray[3];       
      var variantId = variantArray[4];
      var price =  variantArray[5];
      var compareAtPrice = variantArray[6];
      var conversionFactor = $("#product-960479000").data("conversion-factor");                               
      var uom = $("#product-960479000").data("uom");
    
    //console.log("SPECIAL:"+special+"|ADDITIONAL:"+additional+"|PRICE:"+price+"|CAP:"+compareAtPrice+"|CF:"+conversionFactor+"|UOM:"+uom);
      
      if(additional){                
        $("#product-960479000 .additional .sale").html(additional.toUpperCase());
          $("#product-960479000 .additional").show();
      }
      
      $("#product-960479000 .btn").attr('data-id',variantId);
      
      $("#product-960479000 .btn").attr('data-cap',compareAtPrice);
          
      $("#product-960479000 .caption .salecontainer .sale .from_price").html('$' + (variantArray[5]/100).toFixed(2));         
        
        if(special != "Default"){          
          //$("#product-960479000 .caption .salecontainer").addClass("is-special");          
          $("#product-960479000 .caption .unit_price").addClass("is-special");            
        }
            
        if (!compareAtPrice){
          if (uom != "" && uom ){ 
      $("#product-960479000 .caption .salecontainer .sale .compare_at_price").html('<small>$'+((price*conversionFactor)/100).toFixed(2)+" "+uom+'</small>');
          } 
        }else{
        $("#product-960479000 .caption .salecontainer .sale .compare_at_price").html('<small>$' + (variantArray[6]/100).toFixed(2) + ' per kg</small>');              
        }
              
       
          
        if(parseInt(variantArray[7])<=0){
          $("#product-960479000 .ajax-add-to-cart-quick").html("<p class='error'>Out of Stock</p>");          
          $("#product-960479000 a").css({"pointer-events":"none"});          
          $("#product-960479000").remove();
            
         }else{
            $("#product-960479000").addClass("in-stock");
            
            var items = $("#headercartvals").attr("data-items").split(";");
  
            $.each(items, function(index, item) {
              if (item){ 
                  var itemId = item.split("-")[0];
