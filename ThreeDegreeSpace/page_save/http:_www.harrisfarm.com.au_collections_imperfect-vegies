<script>        
  var cookieStore;  
  if($.cookie("picking_store")==null){
    cookieStore = 48;
  }else{
    cookieStore = $.cookie("picking_store");
  }
  
  var pickingStore = "s:"+cookieStore;
  
  for (var i = 0; i < partsOfStr.length; i++) {
    var variantArray = partsOfStr[i].split(',');
  var showProduct = false;
    //console.log("V: "+variantArray[4]+" |DATA: " + variantArray[0] + " |PICK: " + pickingStore);
    
    if(variantArray[0].trim() == pickingStore.trim()){  
      showProduct = true;
      var special =  variantArray[2]; 
      var additional = variantArray[3];       
      var variantId = variantArray[4];
      var price =  variantArray[5];
      var compareAtPrice = variantArray[6];
      var conversionFactor = $("#product-927075621").data("conversion-factor");                               
      var uom = $("#product-927075621").data("uom");
    
    //console.log("SPECIAL:"+special+"|ADDITIONAL:"+additional+"|PRICE:"+price+"|CAP:"+compareAtPrice+"|CF:"+conversionFactor+"|UOM:"+uom);
      
      if(additional){                
        $("#product-927075621 .additional .sale").html(additional.toUpperCase());
          $("#product-927075621 .additional").show();
      }
      
      $("#product-927075621 .btn").attr('data-id',variantId);
      
      $("#product-927075621 .btn").attr('data-cap',compareAtPrice);
          
      $("#product-927075621 .caption .salecontainer .sale .from_price").html('$' + (variantArray[5]/100).toFixed(2));         
        
        if(special != "Default"){          
          //$("#product-927075621 .caption .salecontainer").addClass("is-special");          
          $("#product-927075621 .caption .unit_price").addClass("is-special");            
        }
            
        if (!compareAtPrice){
          if (uom != "" && uom ){ 
      $("#product-927075621 .caption .salecontainer .sale .compare_at_price").html('<small>$'+((price*conversionFactor)/100).toFixed(2)+" "+uom+'</small>');
          } 
        }else{
        $("#product-927075621 .caption .salecontainer .sale .compare_at_price").html('<small>$' + (variantArray[6]/100).toFixed(2) + ' per kg</small>');              
        }
              
       
          
        if(parseInt(variantArray[7])<=0){
          $("#product-927075621 .ajax-add-to-cart-quick").html("<p class='error'>Out of Stock</p>");          
          $("#product-927075621 a").css({"pointer-events":"none"});          
          $("#product-927075621").remove();
            
         }else{
            $("#product-927075621").addClass("in-stock");
            
            var items = $("#headercartvals").attr("data-items").split(";");
  
            $.each(items, function(index, item) {
              if (item){ 
                  var itemId = item.split("-")[0];
                  var itemQty = item.split("-")[1];
                 if(variantArray[4] === itemId){

                     // console.log(variantArray[4] + " in cart");
           $("#product-927075621").find(".ajax-add-to-cart-quick").hide();
                     $("#product-927075621").find(".ajax-update-cart .addToCartInput").val(itemQty);
           $("#product-927075621").find(".ajax-update-cart").show();

                     return;
                       
                  } 
                }
      });
        
            
            
         }      
     }
            
     
                                 
            
   }
                   
       
$(".product-item").not(".in-stock").hide();
              
</script>




        
      </div>
    

  

</div>

