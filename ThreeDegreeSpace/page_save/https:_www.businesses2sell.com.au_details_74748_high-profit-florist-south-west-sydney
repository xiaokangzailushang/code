<script type="text/javascript">
    
	 $(document).ready(function(e) {
        $(".menu_mobile").click(function(e) {
            $(".main_menu").slideToggle(1000);
			});
			 $(".city_menu").click(function(e) {
            $("#header_top").slideToggle(1000);
        
        });
		$(".read_more_text").click(function(e) {
            $('.austraila_content_container p').css('display','block');
			$('.read_more_text').css('display','none');
        });
    });
	
  $(document).ready(function () {
	$(function() {
	  $('a#top_to_up[href*=#]:not([href=#])').click(function() {
		if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
		  var target = $(this.hash);
		  target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
		  if (target.length) {
			$('html,body').animate({
			  scrollTop: target.offset().top
			}, 1000);
			return false;
		  }
		}
	  });
});



$(function(){
        var stickyRibbonTop = $('#header_bottom').offset().top;
        $(window).scroll(function(){
                if( $(window).scrollTop() > stickyRibbonTop ) {
                        $('#header_bottom').css({position: 'fixed', top: '0px' , left : '0px'});
                } else {
                        $('#header_bottom').css({position: 'relative', top: '0px'});
                }
        });
        var stickyRibbonscrol = $('#banner').offset().top;
        stickyRibbonscrol = stickyRibbonscrol + 250;
         $(window).scroll(function(){
                if( $(window).scrollTop() > stickyRibbonscrol ) {
                       	$('#top_to_up').removeClass('top_hide_icon');
                        $('#top_to_up').addClass('go_top');                
                        } 
                        else {
                        $('#top_to_up').removeClass('go_top');
                        $('#top_to_up').addClass('top_hide_icon');    
                }
        });
        
});
});
</script>
